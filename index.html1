  <script>
    // Sound effects for button clicks
    function playClickSound() {
      try {
        // Create audio context for button click sound
        const audioContext = new (window.AudioContext || window.webkitAudioContext)();
        
        // Create oscillator for a very soft whisper-like click sound
        const oscillator = audioContext.createOscillator();
        const gainNode = audioContext.createGain();
        
        oscillator.connect(gainNode);
        gainNode.connect(audioContext.destination);
        
        // Configure sound - very soft whisper tone with consistent volume
        oscillator.type = 'sine';
        oscillator.frequency.setValueAtTime(600, audioContext.currentTime);
        oscillator.frequency.exponentialRampToValueAtTime(400, audioContext.currentTime + 0.2);
        
        // Fixed consistent volume
        gainNode.gain.setValueAtTime(0.03, audioContext.currentTime);
        gainNode.gain.exponentialRampToValueAtTime(0.001, audioContext.currentTime + 0.2);
        
        oscillator.start(audioContext.currentTime);
        oscillator.stop(audioContext.currentTime + 0.2);
      } catch (error) {
        // Silently handle audio context errors
        console.log('Audio not available');
      }
    }

    function playSuccessSound() {
      try {
        // Create audio context for success sound
        const audioContext = new (window.AudioContext || window.webkitAudioContext)();
        
        // Create a gentle, soft success melody
        const times = [0, 0.2, 0.4];
        const frequencies = [440, 523, 659]; // A4, C5, E5 - gentle progression
        
        times.forEach((time, index) => {
          const oscillator = audioContext.createOscillator();
          const gainNode = audioContext.createGain();
          
          oscillator.connect(gainNode);
          gainNode.connect(audioContext.destination);
          
          oscillator.type = 'sine';
          oscillator.frequency.setValueAtTime(frequencies[index], audioContext.currentTime + time);
          
          // Fixed consistent volume for all notes
          gainNode.gain.setValueAtTime(0.025, audioContext.currentTime + time);
          gainNode.gain.exponentialRampToValueAtTime(0.001, audioContext.currentTime + time + 0.6);
          
          oscillator.start(audioContext.currentTime + time);
          oscillator.stop(audioContext.currentTime + time + 0.6);
        });
      } catch (error) {
        // Silently handle audio context errors
        console.log('Audio not available');
      }
    }

    // Mobile menu functionality
    function toggleMobileMenu() {
      playClickSound();
      const mobileMenu = document.getElementById('mobile-menu');
      const menuArrow = document.querySelector('.menu-arrow');
      
      if (mobileMenu.classList.contains('opacity-0')) {
        // Show menu
        mobileMenu.classList.remove('opacity-0', 'scale-95');
        mobileMenu.classList.add('opacity-100', 'scale-100');
        menuArrow.style.transform = 'rotate(180deg)';
      } else {
        // Hide menu
        mobileMenu.classList.remove('opacity-100', 'scale-100');
        mobileMenu.classList.add('opacity-0', 'scale-95');
        menuArrow.style.transform = 'rotate(0deg)';
      }
    }

    function closeMobileMenu() {
      const mobileMenu = document.getElementById('mobile-menu');
      const menuArrow = document.querySelector('.menu-arrow');
      
      mobileMenu.classList.remove('opacity-100', 'scale-100');
      mobileMenu.classList.add('opacity-0', 'scale-95');
      menuArrow.style.transform = 'rotate(0deg)';
    }

    // Close mobile menu when clicking outside
    document.addEventListener('click', function(event) {
      const mobileMenu = document.getElementById('mobile-menu');
      const menuButton = document.querySelector('.mobile-menu-btn');
      
      if (!mobileMenu.contains(event.target) && !menuButton.contains(event.target)) {
        closeMobileMenu();
      }
    });

    // Navigation functionality
    function showSection(sectionId) {
      playClickSound(); // Play sound on navigation
      // Hide all sections
      const sections = document.querySelectorAll('.section-content');
      sections.forEach(section => {
        section.classList.remove('active');
        section.style.display = 'none';
      });
      
      // Show selected section with animation
      const targetSection = document.getElementById(sectionId);
      if (targetSection) {
        targetSection.style.display = 'block';
        setTimeout(() => {
          targetSection.classList.add('active', 'fade-in');
        }, 10);
      }
      
      // Update desktop navigation buttons
      const navButtons = document.querySelectorAll('.nav-btn');
      navButtons.forEach(btn => {
        btn.classList.remove('bg-acc-yellow', 'text-acc-blue');
        btn.classList.add('text-white');
      });
      
      // Update mobile navigation buttons
      const mobileNavButtons = document.querySelectorAll('.nav-btn-mobile');
      mobileNavButtons.forEach(btn => {
        btn.classList.remove('bg-acc-yellow', 'text-white');
        btn.classList.add('text-acc-blue');
      });
      
      // Highlight active button (desktop)
      if (event && event.target.classList.contains('nav-btn')) {
        event.target.classList.remove('text-white');
        event.target.classList.add('bg-acc-yellow', 'text-acc-blue');
      }
      
      // Highlight active button (mobile)
      if (event && event.target.classList.contains('nav-btn-mobile')) {
        event.target.classList.remove('text-acc-blue');
        event.target.classList.add('bg-acc-yellow', 'text-white');
      }
    }
    
    // Form submission handler
    function handleFormSubmit(event) {
      event.preventDefault();
      playSuccessSound(); // Play success sound
      
      // Show success message
      const form = event.target;
      const originalHTML = form.innerHTML;
      
      form.innerHTML = `
        <div class="text-center py-8">
          <div class="text-6xl mb-4">âœ…</div>
          <h3 class="text-2xl font-bold text-acc-blue mb-4">Message Sent Successfully!</h3>
          <p class="text-gray-700 mb-6">Thank you for your interest in Asian Computer College. We'll get back to you within 24 hours.</p>
          <button onclick="resetForm(this)" class="bg-acc-yellow text-acc-blue px-6 py-3 rounded-lg font-bold hover:bg-yellow-400 transition-all duration-300">
            Send Another Message
          </button>
        </div>
      `;
      
      // Store original form HTML for reset
      form.dataset.originalHTML = originalHTML;
    }
    
    // Reset form function
    function resetForm(button) {
      playClickSound(); // Play sound on reset
      const form = button.closest('form');
      form.innerHTML = form.dataset.originalHTML;
    }
    
    // Initialize page
    document.addEventListener('DOMContentLoaded', function() {
      // Show home section by default
      showSection('home');
      
      // Add scroll animations
      const observerOptions = {
        threshold: 0.1,
        rootMargin: '0px 0px -50px 0px'
      };
      
      const observer = new IntersectionObserver((entries) => {
        entries.forEach(entry => {
          if (entry.isIntersecting) {
            entry.target.classList.add('fade-in');
          }
        });
      }, observerOptions);
      
      // Observe all cards for animation
      document.querySelectorAll('.card-hover').forEach(card => {
        observer.observe(card);
      });
    });
    
    // Add some interactive effects
    document.addEventListener('mousemove', function(e) {
      const cards = document.querySelectorAll('.card-hover');
      cards.forEach(card => {
        const rect = card.getBoundingClientRect();
        const x = e.clientX - rect.left;
        const y = e.clientY - rect.top;
        
        if (x >= 0 && x <= rect.width && y >= 0 && y <= rect.height) {
          const centerX = rect.width / 2;
          const centerY = rect.height / 2;
          const rotateX = (y - centerY) / 10;
          const rotateY = (centerX - x) / 10;
          
          card.style.transform = `perspective(1000px) rotateX(${rotateX}deg) rotateY(${rotateY}deg) translateZ(10px)`;
        } else {
          card.style.transform = 'perspective(1000px) rotateX(0deg) rotateY(0deg) translateZ(0px)';
        }
      });
    });
  </script>

